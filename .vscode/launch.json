{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Bottle",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/app.py",
            "env": {
                "SERVER_HOST": "127.0.0.1",
                "SERVER_PORT": "8080",
            },
            "autoReload": {
                "enable": true,
            },
            "justMyCode": true,
        }
    ]
}